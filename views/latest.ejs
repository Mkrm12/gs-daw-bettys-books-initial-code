<!doctype html>
<html>
  <head>
    <title>Welcome to <%= shopData.shopName %></title>
    <link rel="stylesheet" type="text/css" href="/main.css" />
  </head>
  <body>
    <h1><%= shopData.shopName %> Shop</h1>
    <h2>Here are our latest movie releases:</h2>
    <ul>
      <% latestMovies.forEach(function(movie) { %>
        <li><%= movie.title %>, Release Date: <%= movie.releaseDate %></li>
      <% }) %>
    </ul>
  </body>
</html>
